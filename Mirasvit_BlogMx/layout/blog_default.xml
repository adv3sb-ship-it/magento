<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <attribute name="class" value="blog"/>

        <referenceBlock name="wishlist_sidebar" remove="true"/>
        <referenceBlock name="wishlist" remove="true"/>
        <referenceBlock name="catalog.compare.sidebar" remove="true"/>

        <referenceContainer name="main.content" htmlTag="main" htmlId="maincontent" htmlClass="blog-page"/>
        <referenceContainer name="div.sidebar.main" htmlTag="div" htmlClass="aside-content blog-aside-content"/>
        <referenceContainer name="main" htmlClass="container-with_aside flex">
            <container name="blog.main.aside" htmlTag="aside">

            </container>
            <container name="blog.main.content" htmlTag="div" htmlClass="content-with_aside">

            </container>
        </referenceContainer>
        <referenceContainer name="columns" htmlClass="container">
